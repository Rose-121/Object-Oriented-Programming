class Dog:
    
    animal = "Dog"
    
    def __init__(self,name,age,breed, color,angerlevel,):
        self.name = name
        self.age = age
        self.breed = breed
        self.color = color
        self.angerlevel = angerlevel
        
        
Max = Dog("Max", 3, "coloured cute dogüê©", "Golden", 8 )
Jack = Dog("Jack", 4, "German Shepherd ü¶Æ", "Black", 4)


print(f"{Max.name} is a {Max.animal}")
print(f"{Jack.name} is also a {Jack.animal}")

print(f"{Max.name} is {Max.age} years old and is a {Max.color} {Max.breed}.His anger level is {Max.angerlevel} (~_~).")
print(f"{Jack.name} is {Jack.age} years old and is a {Jack.color} {Jack.breed}.His anger level is {Jack.angerlevel}(^_____^).")


 
